Algoritmo "Soma"


Var
   N1, N2, Soma, P : inteiro

Inicio
      escreva ("Digite o primeiro número: ")
      leia (N1)
      escreva ("Digite o segundo número: ")
      leia (N2)
      Soma <- (N1+N2)
      P <- (N1*N2)
      escreval ("A soma dos números é: ", Soma)
      escreva ("O produto é: ", P )


Fimalgoritmo