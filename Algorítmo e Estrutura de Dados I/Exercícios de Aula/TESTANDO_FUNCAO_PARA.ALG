Algoritmo "testando_funcao_para_2"

Var

   valorDeposito, poupanca: real
   nDeposito : inteiro

Inicio

      Para nDeposito <- 1 ate 4 faca
           Escreval ("Digite o valor do deposito", nDeposito, " ")
           Leia (valorDeposito)
           poupanca <- (poupanca + valorDeposito)
      FimPara
      Escreva ("Seu saldo é", poupanca)
      
      
      

Fimalgoritmo