Algoritmo "fatorial"


Var

   n, fat, i: inteiro

Inicio

      Escreva ("Digite um valor")
      Leia (n)
      
      fat <- 1
      
      i <- 1
      Enquanto (i <= n) faca
      
      fat <- fat * i
      i <- i +1
      FimEnquanto
      Escreva ("O fatorial de ", n, " é ", fat)
      

Fimalgoritmo