Algoritmo "exercicio01"
// Thales Mion

Var

   N, D, Q, C, M : real

Inicio

   escreval ("Digite um valor qualquer")
   leia (N)
   D <- (N*2)
   Q <- (N*N)
   C <- (N*N*N)
   M <- (N/2)
   escreval ("O dobro é ", D)
   escreval ("O quadrado é ", Q)
   escreval ("O cubo é ", C)
   escreval ("A metade é ", M)

Fimalgoritmo